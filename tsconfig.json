{
  "include": ["src"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "module": "Node16",
    "moduleResolution": "node16",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true
  }
}
